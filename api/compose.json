{
  "services": {
    "webapi": {
      "build": {
        "context": ".",
        "dockerfile": "Dockerfile"
      },
      "container_name": "webapi",
      "hostname": "webapi",
      "env_file": [
        "./env/api.env"
      ],
      "ports": [
        "8000:8000"
      ],
      "volumes": [
        "./src:/app:rw"
      ]
    }
  }
}
