{
  "services": {
    "mongodb": {
      "image": "mongo:latest",
      "restart": "always",
      "ports": [
        "27017:27017"
      ],
      "env_file": [
        "./env/mongodb.env"
      ],
      "volumes":[
        "./mongodb/data/:/data/db"
      ],
      "command" : "mongod --auth"
    }
  }
}
