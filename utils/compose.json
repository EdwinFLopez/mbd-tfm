{
  "services": {
    "kafka-ui": {
      "container_name": "kafka-ui",
      "hostname": "kafka-ui",
      "image": "provectuslabs/kafka-ui:latest",
      "ports": [
        "8081:8080"
      ],
      "env_file": [
        "./env/kafka-ui.env"
      ],
      "volumes": [
        "./kafka-ui/:/etc/kafkaui/"
      ]
    }
  }
}
