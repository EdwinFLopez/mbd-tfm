{
  "services": {
    "kafka-ui": {
      "container_name": "kafka-ui",
      "hostname": "kafka-ui",
      "image": "provectuslabs/kafka-ui:latest",
      "ports": [
        "9080:8080"
      ],
      "env_file": [
        "./env/kafka-ui.env"
      ],
      "volumes": [
        "./kui/config.yaml:/etc/kafkaui/dynamic_config.yaml"
      ]
    }
  }
}
